<h1>Create New Vendor Sweet</h1>
<%if @errors%>
<%@errors.each do |error|%>
<%=error%>
<%end%>
<%end%>

<%=form_for @vendorsweet do |f|%>
<%=f.label :price%><br>
<%=f.text_field :price%><br>
<%=f.label :vendor_id%>
<%=f.collection_select :vendor_id, Vendor.all, :id, :name %><br>
<%=f.label :sweet_id%>
<%=f.collection_select :sweet_id, Sweet.all, :id, :name%><br>
<%=f.submit%>
<%end%>
